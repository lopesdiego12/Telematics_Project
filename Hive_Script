CREATE TABLE IF NOT EXISTS TELEMATIC.CAR_TELEMATIC_STAGING
(
CARID VARCHAR(255),
AGE	INT,
AVGVELOCITY INT,
GENDER CHAR(1),
CRASH CHAR(1),
KM INT,
MARCHA CHAR(1),
ROAD CHAR(1),
CLASS VARCHAR(255)
)
COMMENT 'Telematic car data'
ROW FORMAT DELIMITED
FIELDS TERMINATED BY ','
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;

load data inpath '/sourcedata_telematics/data_1.csv' into table car_telematic
