CREATE EXTERNAL TABLE IF NOT EXISTS telematic.car
(CARID VARCHAR(255),
AGE	INT,
data DATE,
AVGVELOCITY INT,
EMAIL VARCHAR(255),
GENDER CHAR(1),
CRASH CHAR(1),
KM INT,
MARCHA CHAR(1),
RODOVIA CHAR(1),
LONGITUDE INT,
LATITUDE INT)
COMMENT 'Telematic car data'
ROW FORMAT DELIMITED
FIELDS TERMINATED BY ','
LINES TERMINATED BY '\n'
STORED AS TEXTFILE
LOCATION '/sourcedata_telematics/';


Unix script
cat dataset_4.csv | sed "s/'/ /g" | sed -r 's/(\[|\])//g' | sed 's/ //g' >> data_4.csv
